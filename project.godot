config_version=4

[application]

config/name="Drill Game"
config/description="A game about drilling"
run/main_scene="res://scenes/start/Start.tscn"
boot_splash/image="res://assets/images/splash.png"
boot_splash/bg_color=Color( 0.141176, 0.141176, 0.141176, 1 )
config/icon="res://icon.png"

[audio]

default_bus_layout="res://bus_layout.tres"

[autoload]

Globals="*res://scripts/globals.gd"
EffectsPlayer="*res://scripts/effects_player.gd"

[debug]

settings/fps/force_fps=60

[display]

window/size/width=320
window/size/height=240
window/size/test_width=1280
window/size/test_height=960
window/stretch/mode="2d"
window/stretch/aspect="keep"

